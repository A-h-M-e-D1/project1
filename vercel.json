{
    "builds": [
      {
        "src": "project1/wsgi.py",
        "use": "@vercel/python",
        "config":{"maxLambdaSize":"15mb","runtime":"python3.11"}
      }
    ],
    "routes": [
      { "handle": "filesystem" },
      { "src": "/.*", "dest": "project1/wsgi.py" }
    ]
  }
  